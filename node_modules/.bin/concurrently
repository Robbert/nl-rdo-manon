#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/nl-rdo-manon/nl-rdo-manon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/nl-rdo-manon/nl-rdo-manon/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/concurrently@7.2.2/node_modules/concurrently/dist/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/concurrently@7.2.2/node_modules/concurrently/dist/bin/concurrently.js" "$@"
fi
